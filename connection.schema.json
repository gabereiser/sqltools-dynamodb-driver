{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "region": {
      "title": "AWS region",
      "type": "string",
      "minLength": 1
    },
    "accessKeyId": {
      "title": "Access key ID",
      "type": "string",
      "minLength": 1
    },
    "secretAccessKey": {
      "title": "Secret access key",
      "type": "string",
      "minLength": 1
    },
    "filter": {
      "title": "Table pattern",
      "type": "string",
      "minLength": 1
    }
  },
  "required": ["region", "accessKeyId", "secretAccessKey"]
}
