{
  "name": "sqltools-dynamodb-driver",
  "displayName": "DynamoDB Driver",
  "description": "DynamoDB Driver to be used as base for new VSCode SQLTools drivers",
  "version": "0.1.3",
  "icon": "icons/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/khanghua-itr/sqltools-dynamodb-driver"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "publisher": "khanghua",
  "license": "MIT",
  "bugs": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [
    "sqltools-dynamodb-driver",
    "sqltools-driver"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.284.0",
    "@sqltools/base-driver": "latest",
    "@sqltools/types": "latest",
    "dynamodb-data-types": "^4.0.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "typescript": "^3.7.3"
  }
}
